{
    "routes": [
        {
            "route":"/login",
            "redirect":"/.auth/login/githb"
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
        },
        {
            "route": "/named-dogs",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/api/named-dogs",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/login"
        }
        "404": {
            "statusCode": 301,
            "redirect": "/login"
        }
    }
}
